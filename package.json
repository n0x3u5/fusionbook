{
  "name": "fusionbook",
  "version": "0.0.1",
  "description": "Develop FusionCharts components in isolation",
  "main": "src/client/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf ./dist",
    "bundle": "rollup -c",
    "look": "rollup -c -w",
    "build:prod": "npm run clean && cross-env NODE_ENV=production BUILD_TYPE=build npm run bundle",
    "build:dev": "npm run clean && cross-env NODE_ENV=development BUILD_TYPE=build npm run bundle",
    "watch:prod": "npm run clean && cross-env NODE_ENV=production BUILD_TYPE=watch npm run look",
    "watch:dev": "npm run clean && cross-env NODE_ENV=development BUILD_TYPE=watch npm run look",
    "start:prod": "cross-env SERVED=true npm run watch:prod",
    "start:dev": "cross-env SERVED=true npm run watch:dev",
    "build": "npm run build:prod",
    "watch": "npm run watch:prod",
    "start": "npm run start:prod",
    "lint": "npx eslint ./src ./stories ./components"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n0x3u5/fusionbook.git"
  },
  "keywords": [
    "fusioncharts"
  ],
  "author": "Nakshatra Mukhopadhyay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/n0x3u5/fusionbook/issues"
  },
  "homepage": "https://github.com/n0x3u5/fusionbook#readme",
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "acorn": "^6.1.1",
    "babel-loader": "^8.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^3.4.2",
    "eslint": "^5.15.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-copy": "^7.1.0",
    "rimraf": "^2.6.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-copy": "^3.1.0",
    "rollup-plugin-generate-html": "^0.2.0",
    "rollup-plugin-livereload": "^1.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-serve": "^1.0.1",
    "rollup-plugin-typescript2": "^0.23.0",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "typescript": "^3.6.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "lodash-es": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-inspector": "^2.3.1",
    "rollup": "^1.20.2",
    "semantic-ui-css": "^2.4.1",
