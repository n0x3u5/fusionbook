{
  "name": "fusionbook",
  "version": "0.0.1",
  "description": "Develop FusionCharts components in isolation",
  "main": "src/client/index.js",
  "scripts": {
    "test": "npm run lint && npm run build",
    "clean": "rimraf ./dist",
    "bundle": "webpack",
    "look": "webpack -w",
    "serve": "webpack-dev-server",
    "build:prod": "npm run clean && cross-env NODE_ENV=production npm run bundle",
    "build:dev": "npm run clean && cross-env NODE_ENV=development npm run bundle",
    "build": "npm run build:prod",
    "watch:prod": "npm run clean && cross-env NODE_ENV=production npm run look",
    "watch:dev": "npm run clean && cross-env NODE_ENV=development npm run look",
    "watch": "npm run watch:prod",
    "start:prod": "cross-env NODE_ENV=production npm run serve",
    "start:dev": "cross-env NODE_ENV=development npm run serve",
    "start": "npm run start:prod",
    "lint:scripts": "eslint ./src ./stories ./components",
    "lint:styles": "stylelint ./src",
    "lint": "npm run lint:scripts && npm run lint:styles"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n0x3u5/fusionbook.git"
  },
  "keywords": [
    "fusioncharts"
  ],
  "author": "Nakshatra Mukhopadhyay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/n0x3u5/fusionbook/issues"
  },
  "homepage": "https://github.com/n0x3u5/fusionbook#readme",
  "devDependencies": {
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-inspector": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "rimraf": "^3.0.0",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^19.0.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "@fusioncharts/core": "^1.0.2",
    "react": "0.0.0-experimental-f42431abe",
    "react-dom": "0.0.0-experimental-f42431abe",
    "react-inspector": "^2.3.1"
  }
}
